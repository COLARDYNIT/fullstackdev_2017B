entity Device {
	name String,
    state Boolean,
    type Type
}

enum Type {
	LIGHT,
    POWER_SOURCE,
    SHUTTERS,
    AUDIO_VOLUME
}

entity Mood {
	name String,
    active Boolean,
    audioVolume Integer,
    brightness Integer,
    shutterHeight Integer,
}


relationship ManyToMany {
	Mood{Device} to Device{Mood}
}


paginate all with pager
service all with serviceImpl
