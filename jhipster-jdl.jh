entity Device {
	name String,
    state Boolean,
    type Type
}

entity DeviceInState{
    state Boolean,
}

enum Type {
	LIGHT,
    POWER_SOURCE,
    SHUTTERS,
    AUDIO_VOLUME
}

entity Mood {
	name String,
    active Boolean,
    audioVolume Integer,
    brightness Integer,
    shutterHeight Integer,
}
relationship ManyToOne{
    DeviceInState{Device} to Device
}

relationship ManyToMany {
	Mood{DeviceInState} to DeviceInState{Mood}
}


paginate all with pager
service all with serviceImpl
